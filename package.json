{
  "name": "tomer.link",
  "private": true,
  "author": "Tomer Aberbach <tomeraberbach@gmail.com> (https://tomeraberba.ch)",
  "description": "A Netlify-based personal URL shortener that generating funny adjective-animal phrases.",
  "keywords": [
    "netlify",
    "url-shortener",
    "url",
    "redirect",
    "random-generator",
    "animals"
  ],
  "homepage": "https://github.com/TomerAberbach/tomer.link",
  "repository": "TomerAberbach/tomer.link",
  "bugs": {
    "url": "https://github.com/TomerAberbach/tomer.link/issues"
  },
  "license": "MIT",
  "scripts": {
    "lint": "prettier --ignore-path .gitignore --write '{{{src,test}/**,.}/*.{js,json,md},.{prettier,babel}rc}'",
    "test": "jest --testMatch **/test/**/*.js",
    "build": "babel -D src -d lib",
    "clean": "rimraf lib",
    "start": "node lib/index.js"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.8.4",
    "binary-search": "^1.3.6",
    "clipboardy": "^2.1.0",
    "execa": "^4.0.0",
    "fs-extra": "^8.1.0",
    "mem": "^6.0.1",
    "pkg-up": "^3.1.0",
    "random-number-csprng": "^1.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@tomera/babel-preset-node": "^2.0.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.1"
  }
}
