{
  "name": "tomer.link",
  "private": true,
  "author": "Tomer Aberbach <tomeraberbach@gmail.com> (https://tomeraberba.ch)",
  "homepage": "https://github.com/TomerAberbach/tomer.link",
  "repository": "TomerAberbach/tomer.link",
  "bugs": {
    "url": "https://github.com/TomerAberbach/tomer.link/issues"
  },
  "license": "MIT",
  "scripts": {
    "lint": "prettier --ignore-path .gitignore --write '{{{src,test}/**,.}/*.{js,json,md},.{prettier,babel}rc}'",
    "test": "jest --testMatch **/test/**/*.js",
    "build:js": "babel src -d lib",
    "build:json": "cpy '**/*.json' '../lib' --cwd=src --parents",
    "build": "run-p build:*",
    "clean": "rimraf lib",
    "start": "node lib/index.js"
  },
  "dependencies": {
    "binary-search": "^1.3.6",
    "execa": "^4.0.0",
    "fs-extra": "^8.1.0",
    "mem": "^6.0.1",
    "pkg-up": "^3.1.0",
    "random-number-csprng": "^1.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/runtime-corejs3": "^7.8.4",
    "@tomera/babel-preset-node": "^1.0.0",
    "cpy-cli": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.1"
  }
}
